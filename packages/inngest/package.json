{
  "name": "inngest-kit",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./functions": {
      "types": "./dist/functions/index.d.ts",
      "default": "./dist/functions/index.js"
    },
    "./adapters/hono": {
      "types": "./dist/adapters/hono.d.ts",
      "default": "./dist/adapters/hono.js"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "default": "./dist/events.js"
    },
    "./client.browser": {
      "types": "./dist/client.browser.d.ts",
      "default": "./dist/client.browser.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "inngest": "^3.43.1",
    "hono": "^4.9.9",
    "honoken": "workspace:*",
    "database": "workspace:*"
  },
  "devDependencies": {
    "inngest": "^3.43.1"
  }
}
